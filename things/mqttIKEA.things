////Luči
//otok
Thing mqtt:topic:mosquitto:otokMalaLuc "Luč mala otok" (mqtt:broker:mosquitto) @ "Kuhinja" [ availabilityTopic="zigbee2mqtt/lightMalaOtok/availability", payloadAvailable="{\"state\":\"online\"}", payloadNotAvailable="{\"state\":\"offline\"}" ] {
    Channels:
        Type number : brightness    "svetlost"      [ stateTopic = "zigbee2mqtt/lightMalaOtok/brightness", commandTopic = "zigbee2mqtt/lightMalaOtok/set/brightness" ]
        Type number : linkQuality   "povezava"      [ stateTopic = "zigbee2mqtt/lightMalaOtok/linkquality" ]
        Type number : colorTemp     "barva luči"    [ stateTopic = "zigbee2mqtt/lightMalaOtok/color_temp", commandTopic = "zigbee2mqtt/lightMalaOtok/set/color_temp" ]
        Type switch : state         "stanje"        [ stateTopic = "zigbee2mqtt/lightMalaOtok/state", commandTopic = "zigbee2mqtt/lightMalaOtok/set/state", on="on", off="off" ]
        Type switch : update        "posodobitev"   [ stateTopic = "zigbee2mqtt/lightMalaOtok/update_available", on="true", off="false" ]
        Type switch : availability  "availability"  [ stateTopic = "zigbee2mqtt/lightMalaOtok/availability", on="{\"state\":\"online\"}", off="{\"state\":\"offline\"}" ]
}

